{
  "name": "angular-chartist.js",
  "version": "5.0.0-beta.0",
  "description": "Angular directive for Chartist.js",
  "main": "dist/angular-chartist.js",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "karma start",
    "test:watch": "npm run test -- --no-single-run --auto-watch",
    "lint": "eslint src/ test/",
    "prepare": "npm run build",
    "postpublish": "git push origin master --follow-tags",
    "start": "webpack-serve",
    "prerelease": "npm test",
    "release": "standard-version --sign",
    "postrelease": "git push --follow-tags origin master && npm publish",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/willsoto/angular-chartist.js"
  },
  "keywords": [
    "angular",
    "chartist.js",
    "directive"
  ],
  "author": "Will <will.soto9+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willsoto/angular-chartist.js/issues"
  },
  "homepage": "https://github.com/willsoto/angular-chartist.js",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-2": "7.0.0-beta.44",
    "@babel/register": "7.0.0-beta.44",
    "angular": "^1.7.1",
    "angular-mocks": "^1.7.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-angularjs-annotate": "^0.9.0",
    "chai": "^4.1.2",
    "chartist": "^0.11.0",
    "conventional-changelog": "^2.0.1",
    "conventional-changelog-cli": "^2.0.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "greenkeeper-lockfile": "1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "isparta-loader": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "lint-staged": "^7.1.3",
    "mocha": "^5.2.0",
    "prettier": "^1.13.5",
    "puppeteer": "^1.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^4.0.0-beta.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-filesize": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "standard-version": "^4.4.0",
    "style-loader": "^0.21.0",
    "validate-commit": "^3.4.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-serve": "^1.0.4"
  },
  "peerDependencies": {
    "angular": ">=1.2.28",
    "chartist": ">=0.9.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
