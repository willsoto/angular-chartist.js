{
    "rules": {
        "quotes": [2, "single", "avoid-escape"],
        "camelcase": [1, {
            "properties": "never"
        }],
        "no-underscore-dangle": 0,
        "no-shadow": [1, {
            "hoist": "never"
        }],
        "no-use-before-define": 0,
        "no-new": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
        "modules": true
    }
}
