{
    "preset": "google",
    "esnext": true,
    "validateIndentation": 4,
    "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties",
    "maximumLineLength": 140,
    "validateQuoteMarks": {
        "mark": "'",
        "escape": true
    },
    "excludeFiles": ["node_modules/**", "app/bower_components/**"]
}
